{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "ses:SendEmail",
      "Resource": ["arn:aws:ses:*:*:identity/*", "arn:aws:ses:*:*:configuration-set/first-setting"],
      "Condition": {
        "StringLike": {
          "ses:FromAddress": "*"
        }
      }
    }
  ]
}
